<?xml version="1.0" encoding="utf-8"?>
<data noupdate="1">
    <record id="view_field_service_specialty_list" model="ir.ui.view">
        <field name="name">field.service.specialty.list</field>
        <field name="model">field.service.specialty</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="create_date"/>
                <field name="create_uid"/>
            </list>
        </field>
    </record>

    <record id="view_field_service_specialty_form" model="ir.ui.view">
        <field name="name">field.service.specialty.form</field>
        <field name="model">field.service.specialty</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="create_date" readonly="1"/>
                        <field name="create_uid" readonly="1"/>
                    </group>
                </sheet>
                <chatter/>
            </form>
        </field>
    </record>
    <menuitem id="menu_field_service_config" name="Especialidades y Tabulador" parent="industry_fsm.fsm_menu_settings" sequence="10"/>

    <record id="action_field_service_specialty" model="ir.actions.act_window">
        <field name="name">Especialidades</field>
        <field name="res_model">field.service.specialty</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_field_service_specialty"
              name="Especialidades"
              parent="menu_field_service_config"
              action="action_field_service_specialty"
              sequence="10"/>
</data>
