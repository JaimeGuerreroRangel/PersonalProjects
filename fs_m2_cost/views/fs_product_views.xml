<?xml version="1.0" encoding="utf-8"?>
<data noupdate="1">
    <record id="view_product_template_form_fs_inherit" model="ir.ui.view">
        <field name="name">product.template.form.field.service.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/notebook" position="inside">
                <page string="Field Service">
                    <group invisible="type != 'service'">
                        <field name="fs_specialty_id"/>
                        <field name="fs_service_type_id"/>
                        <field name="fs_material_template_id"/>
                    </group>
                    <group string="Tipo de servicio FS">
                        <field name="fs_is_installation"/>
                        <field name="fs_is_leveling"/>
                        <field name="fs_is_evaluation"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
    
</data>
